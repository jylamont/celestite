{
  "name": "celestite",
  "version": "0.1.2",
  "repository": "https://github.com/noahlh/crystal-vue",
  "author": "Noah Lehmann-Haupt <nlh@nlh.me>",
  "license": "MIT",
  "scripts": {
    "vue-build": "rm -rf build/ && npx babel src/server -d build/server",
    "vue-dev": "NODE_ENV=development npx nodemon --exec node build/server/render-server.js",
    "vue-debug": "NODE_ENV=development npx nodemon --exec node --inspect-brk build/server/render-server.js",
    "vue-prod": "NODE_ENV=production node build/server/render-server.js",
    "prevue-dev": "yarn build",
    "prevue-prod": "yarn build",
    "prevue-debug": "yarn build",
    "svelte-dev": "sapper dev --no-hot",
    "svelte-build": "sapper build",
    "svelte-prod": "node svelte-scripts/__sapper__/build"
  },
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "body-parser": "^1.19.0",
    "css-loader": "^2.0.1",
    "express": "^4.17.1",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.2",
    "polka": "next",
    "sass-loader": "^8.0.0",
    "vue": "^2.5.21",
    "vue-loader": "^15.4.2",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.5.21",
    "vue-template-compiler": "^2.5.21",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.6.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.5",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.9.0"
  }
}
